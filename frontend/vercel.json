{
  "functions": {
    "app/api/route.ts": {
      "maxDuration": 30
    }
  },
  "outputDirectory": ".next",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "public": true,
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/images/personality-animals/:path*",
      "destination": "/images/personality-animals/:path*"
    }
  ]
}# Force rebuild 20250820203431
